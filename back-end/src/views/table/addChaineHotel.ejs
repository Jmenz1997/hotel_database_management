<form action="/addChaineHotel" method="POST">
    <label for="nom_chaine">Name:</label>
    <input type="text" id="nom_chaine" name="nom_chaine" required>
    <br>
    <label for="adresse_bureau">Adresse bureau:</label>
    <input type="text" id="adresse_bureau" name="adresse_bureau" required>
    <br>
    <label for="tel">Numéro téléphone:</label>
    <input type="number" id="tel" name="tel" required>
    <br>
    <label for="mail">email:</label>
    <input type="email" id="email" name="email" required>
    <br>
    <label for="nombre_hotels">Nombre d'hotel:</label>
    <input type="number" id="nombre_hotels" name="nombre_hotels" required>
    <br>
    <button type="submit">Add Chaine Hotel</button>
  </form>
  